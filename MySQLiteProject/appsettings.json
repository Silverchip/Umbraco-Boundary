{
  "$schema": "./appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "Umbraco": {
    "CMS": {
      "Global": {
        "Id": "a872afe6-6f68-4114-94bb-1dac65162a91",
        "SanitizeTinyMce": true
      },
      "Content": {
        "AllowEditInvariantFromNonDefault": true,
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        }
      }
    },
    "Forms": {
      "FieldTypes": {
        "Recaptcha2": {
          "PublicKey": "",
          "PrivateKey": ""
        },
        "Recaptcha3": {
          "PublicKey": "",
          "PrivateKey": ""
        }
      }
    }
  },
  "uSkinned": {
    "SourceCodeOptions": "uSkinned",
    "NotPages": "USNWebsiteConfigurationSection,USNGlobalSettings,USNFooter,USNNavigation,USNStylesFolder,USNStyle,USNBlogAuthor,USNBlogAuthors,USNBlogCategories,USNBlogMonthFolder,USNBlogPostsFolder,USNBlogYearFolder,USNReusablePods,USNReusableComponents,USNComponentGroup,USNPodGroup",
    "HideFromPath": "USNBlogCategories,USNBlogPostsFolder",
    "WebPEnabled": true
  }
}